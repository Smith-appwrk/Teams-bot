{
    "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.19/MicrosoftTeams.schema.json",
    "manifestVersion": "1.19",
    "version": "1.0.0",
    "id": "5a6be6c1-a536-48ca-ab03-38d75c0d5b43",
    "developer": {
        "name": "IntelliGate Support",
        "websiteUrl": "https://sdcprod.unilever.com/",
        "privacyUrl": "https://www.microsoft.com/privacy",
        "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
    },
    "name": {
        "short": "IntelliGate AI Agent",
        "full": "IntelliGate Support AI Assistant"
    },
    "description": {
        "short": "AI-powered assistant for IntelliGate support and troubleshooting.",
        "full": "An intelligent bot that helps users with IntelliGate-related questions, troubleshooting, and support. Provides instant answers about driver registration, QR code issues, application problems, yard management, and common error messages."
    },
    "icons": {
        "outline": "outline.png",
        "color": "color.png"
    },
    "bots": [
        {
            "botId": "707541ed-e6ce-4f2a-adb8-bb01091d6b37",
            "scopes": [
                "team",
                "personal",
                "groupChat"
            ],
            "isNotificationOnly": false
        }
    ],
    "accentColor": "#60A18E",
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "hello",
        "token.botframework.com"
    ],
    "webApplicationInfo": {
        "id": "707541ed-e6ce-4f2a-adb8-bb01091d6b37",
        "resource": "https://AnyString"
    },
    "authorization": {
        "permissions": {
            "resourceSpecific": [
                {
                    "type": "Application",
                    "name": "ChannelMessage.Read.Group"
                },
                {
                    "type": "Application",
                    "name": "ChatMessage.Read.Chat"
                }
            ]
        }
    }
}